      "path": "/api/Inventory/GetInventoryItems",
      "summary": "Retrieve inventory items with filtering and pagination",
      "auth": true,
      "parameters": [
        {"name": "VIN", "type": "string", "in": "query", "required": false},
        {"name": "SerialNumber", "type": "string", "in": "query", "required": false},
        {"name": "VehicleVersionId", "type": "integer", "in": "query", "required": false},
