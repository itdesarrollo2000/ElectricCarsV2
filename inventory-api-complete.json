        {"name": "MinMileage", "type": "integer", "in": "query", "required": false},
        {"name": "MaxMileage", "type": "integer", "in": "query", "required": false},
        {"name": "ModelYear", "type": "integer", "in": "query", "required": false},
        {"name": "EntryDateFrom", "type": "string", "format": "date-time", "in": "query", "required": false},
        {"name": "EntryDateTo", "type": "string", "format": "date-time", "in": "query", "required": false},
        {"name": "SupplierName", "type": "string", "in": "query", "required": false},
        {"name": "HasExited", "type": "boolean", "in": "query", "required": false},
        {"name": "PageSize", "type": "integer", "in": "query", "required": false},
        {"name": "PageNumber", "type": "integer", "in": "query", "required": false}
